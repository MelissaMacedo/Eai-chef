{
  "version": 2,
  "builds": [
    {
      "src": "super_urls.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "super_urls.py" 
    }
  ],
  "env": {
    "SECRET_KEY": "@receitas_ai_secret_key"
  }
}